{
  "name": "Google Classic Results",
  "version": "1.0",
  "description": "Cleans up google results by using the old classic style with green links",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "https://google.com/"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "run_at": "document_end",
      "js": ["content/google.js"],
      "css": ["content/google.css"]
    }
  ]
}